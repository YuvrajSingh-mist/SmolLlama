    "import re\n",
    "def clean_prompt(text):\n",
    "    \n",
    "    text = re.sub(r'### Instruction:\\s*', '', text)\n",
    "    text = re.sub(r'### Input:\\s*', '', text)\n",
    "    text = re.sub(r'### Response:\\s*', '', text)\n",
    "    return text.strip()\n",
    "\n",
    "clean_prompt(\"### Instruction: Hi ### Input: My name is \").strip()\n"
   ]
